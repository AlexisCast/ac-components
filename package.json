{  
  "name": "@castalexis/ac-components",
  "author": "AlexisCast",
  "module": "dist/ac-components.esm.js",
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build-storybook": "storybook build",
    "build": "rollup -c",
    "build:rollup": "tsdx build",
    "clear": "rm -rf node_modules/ & rm -rf dist/ & rm -rf package-lock.json  & rm -rf storybook-static",
    "clear:dist": "rm -rf dist/",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "lint:fix": "eslint src/ --fix",
    "lint": "eslint --ext ts,tsx .",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "storybook": "storybook dev -p 6006",
    "test": "tsdx test --passWithNoTests"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/ac-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ac-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-styling": "^1.3.5",
    "@storybook/addons": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-webpack5": "^7.1.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup": "^2.78.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "size-limit": "^8.2.6",
    "storybook": "^7.1.0",
    "tailwindcss": "^3.3.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.0",
    "typescript": "^3.9.10"
  },
  "homepage": "http://AlexisCast.github.io/ac-components",
  "dependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "classnames": "^2.3.2"
  }
}
